  "contributors": [
    "Thomas Vanderhaeghen <info@tmas.me> (http://www.tmas.me)"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://git@github.com/SGoerzen/moment-round-es.git"
  },
  "scripts": {
